<div style="padding: 22px; max-width:600px; align-items: center;">
  <p id="notice"><%= notice %></p>

  <h1>Daily Priorities</h1>

  <table class="index-table">
    <thead>
      <tr>
        <th>Initial</th>
        <th>One</th>
        <th>Oneexecuted</th>
        <th>Two</th>
        <th>Twoexecuted</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @daily_priorities.each do |daily_priority| %>
        <tr>
          <td><%= daily_priority.initial %></td>
          <td><%= daily_priority.one %></td>
          <td><%= daily_priority.oneexecuted %></td>
          <td><%= daily_priority.two %></td>
          <td><%= daily_priority.twoexecuted %></td>
          <td><%= link_to 'Show', daily_priority %></td>
          <td><%= link_to 'Edit', edit_daily_priority_path(daily_priority) %></td>
          <td><%= link_to 'Destroy', daily_priority, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'D Daily Form', daily_priorities_d_form_path, class: "btn btn-primary" %>
  <%= link_to 'N Daily Form', daily_priorities_n_form_path, class: "btn btn-primary" %>
</div>